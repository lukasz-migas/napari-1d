include LICENSE
include README.md
recursive-include napari_1d *.pyi
recursive-include napari_1d _tests/*.py
recursive-include napari_1d *.pyi
recursive-include napari_1d *.png *.svg *.qss *.gif *.ico *.icns
recursive-include napari_1d *.yaml
exclude napari_1d/resources/_qt_resources*.py

exclude requirements.txt

# explicit excludes to keep check-manifest happy and remind us that
# these things are not being included unless we ask
recursive-exclude tools *
recursive-exclude docs *
recursive-exclude examples *
recursive-exclude misc *
recursive-exclude napari_1d __pycache__
recursive-exclude napari_1d *.pyc
exclude napari_1d/benchmarks/*
recursive-exclude binder *
exclude bundle.py
exclude dockerfile
exclude napari_1d _version.py
exclude mkdocs.yml